  <refnamediv xml:id="knearest_sci">
    <refname>knearest_sci</refname>

    <refpurpose>K-Nearest Neighbors</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <title>Calling Sequence</title>

    <synopsis>test_labels = knearest_sci(train_data, train_labels, test_data, k)</synopsis>
  </refsynopsisdiv>

  <refsection>
    <title>Parameters</title>

    <variablelist>
      <varlistentry>
        <term>train_data</term>

        <listitem>
          <para>dim*num data matrix; each column is a data point.</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>train_labels</term>

        <listitem>
          <para>labels of each train_data.</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>test_data</term>

        <listitem>
          <para>dim*t_num data matrix; each column is a data point.</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>k</term>

        <listitem>
          <para>number of nearest neighbors</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>test_labels</term>

        <listitem>
          <para>labels of each test_data.</para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsection>

