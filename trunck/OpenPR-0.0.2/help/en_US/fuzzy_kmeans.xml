  <refnamediv xml:id="fuzzy_kmeans">
    <refname>fuzzy_kmeans</refname>

    <refpurpose>Fuzzy K-Means Algorithm</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <title>Calling Sequence</title>

    <synopsis>[labels, centroids] = fuzzy_kmeans(data, k, b)</synopsis>
  </refsynopsisdiv>

  <refsection>
    <title>Parameters</title>

    <variablelist>
      <varlistentry>
        <term>data</term>

        <listitem>
          <para>dim*num data matrix; each column is a data point.</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>k</term>

        <listitem>
          <para>number of nearest neighbors.</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>b</term>

        <listitem>
          <para>b &gt; 1 is a free parameter chosen to adjust the “blending”
          of different clusters</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>labels</term>

        <listitem>
          <para>labels of the input data.</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>centroids</term>

        <listitem>
          <para>cluster centroids</para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsection>

