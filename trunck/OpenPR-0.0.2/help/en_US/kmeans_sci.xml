  <refnamediv>
    <refname>kmeans_sci</refname>

    <refpurpose>K-Means clustering</refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <title>Calling Sequence</title>

    <synopsis>[labels, centroids] = kmeans_sci(data, k, stop)</synopsis>
  </refsynopsisdiv>

  <refsection>
    <title>Parameters</title>

    <variablelist>
      <varlistentry>
        <term>data</term>

        <listitem>
          <para>dim*num data matrix; each column is a data point.</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>k</term>

        <listitem>
          <para>number of nearest neighbors.</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>stop</term>

        <listitem>
          <para>a scalar in (0, 1). Stop iterations if improved rate is less
          than this value.</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>labels</term>

        <listitem>
          <para>labels of the input data.</para>
        </listitem>
      </varlistentry>
    </variablelist>

    <variablelist>
      <varlistentry>
        <term>centroids</term>

        <listitem>
          <para>cluster centroids</para>
        </listitem>
      </varlistentry>
    </variablelist>
  </refsection>

